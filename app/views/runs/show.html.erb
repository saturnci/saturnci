<%= render @project_component do |component| %>
  <%= component.with_body do %>
    <div class="dashboard">
      <%= render "builds/list", current_tab_name: @current_tab_name %>

      <div class="build">
        <%= render "runs/detail",
          current_tab_name: @current_tab_name,
          run_output_stream: @run_output_stream %>
      </div>
    </div>
  <% end %>
<% end %>
