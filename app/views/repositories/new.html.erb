<div class="container">
  <h1>Add Repository</h1>
  <% @github_repositories.each do |github_repository| %>
    <div>
      <%= link_to github_repository.full_name,
        repositories_path(repo_full_name: github_repository.full_name),
        data: { turbo_method: :post } %>
    </div>
  <% end %>
</div>
